{
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.18.0",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev -o",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --fix",
    "typecheck": "nuxi typecheck",
    "check": "pnpm lint && pnpm typecheck",
    "test": "vitest"
  },
  "dependencies": {
    "@nuxt/content": "^3.1.1",
    "@nuxt/fonts": "^0.11.0",
    "@nuxt/image": "^1.8.1",
    "@nuxt/ui": "^3.0.2",
    "@vueuse/nuxt": "^13.0.0",
    "better-sqlite3": "^12.4.1",
    "magic-regexp": "^0.10.0",
    "motion-v": "^1.2.0",
    "nuxt": "^3.17.5",
    "nuxt-og-image": "^5.1.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.0.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@damisparks/eslint-config": "^2.0.0",
    "@iconify-json/heroicons": "^1.2.2",
    "@iconify-json/lucide": "^1.2.39",
    "@iconify-json/simple-icons": "^1.2.13",
    "@nuxt/eslint": "1.9.0",
    "@nuxt/test-utils": "^3.19.2",
    "@tailwindcss/typography": "^0.5.16",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.12.0",
    "eslint-plugin-format": "^1.0.0",
    "happy-dom": "^18.0.1",
    "lint-staged": "^16.0.0",
    "nuxt-gtag": "^3.0.1",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.6.3",
    "vitest": "^3.2.4",
    "vue": "^3.5.13",
    "vue-tsc": "^3.0.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3",
      "sharp",
      "@tailwindcss/oxide",
      "unrs-resolver",
      "simple-git-hooks"
    ],
    "ignoredBuiltDependencies": [
      "vue-demi",
      "esbuild",
      "@parcel/watcher"
    ]
  },
  "resolutions": {
    "unimport": "5.4.1"
  },
  "simple-git-hooks": {
    "commit-msg": "npx commitlint --edit $1",
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,mjs,vue}": [
      "eslint"
    ]
  }
}
